import  java.awt.*;
public class CenterPanel2{
    public static void main(String[] args){
        new MyFrame4("Frame with Panel",0,0,200,300);
    }
}

class MyFrame4 extends Frame{
    private Panel p1;
    MyFrame4(String s,int x, int y, int w, int h){
        super(s);
        setLayout(null);
        setBounds(x,y,w,h);
        setBackground(Color.BLUE);
        p1 = new Panel(null);
        p1.setBounds(w/4,h/4,w/2,h/2);
        p1.setBackground(Color.YELLOW);
        add(p1);
        setVisible(true);
    }
}
